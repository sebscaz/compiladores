		crear entero sebas;
		crear entero galaxy;
		crear entero areglo[3][10];
		crear entero areglo2[4];
		crear entero celular;		

		sebas = 1;
		galaxy = 4;
		sebas = 2 + 3 / (4 * 3) - 4;
		celular = 2;
		
		dibujarFigura(perro, prueba1);

		si(4>3){
			si(5>2){
				imprimir("if anidado");
				sebas = leer();
			}
			imprimir("hola");
			sebas = 1 * 2;
		} no {
			imprimir("adios");
			sebas = 4 / 2;
		}
		sebas = 3 + 3;
		areglo[0][1]=1;
		sebas = areglo[0][1];
		regresar 3
		
	}


