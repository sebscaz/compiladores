programa test2 {
	prototipo factorial(1);

	crear entero varglobal;
	varglobal=1;

	funcion entero factorial(entero x){

		crear entero dos;
		crear entero uno;
		crear entero y;
		crear entero respuesta;

		dos=2;
		uno=1;
		y = x + 1;
		respuesta = 1;

		si(x < dos){
			regresar uno
		}
	
		mientras(dos < y ) hacer {
			respuesta = respuesta * dos;
			dos = dos + 1;
		}

		regresar respuesta
	}

	principal(){
		crear entero numeroInicial;
		crear entero respuesta;
		crear entero dos;
		crear entero uno;
		crear entero y;

		numeroInicial = 5;
		respuesta = 0;
		dos=2;
		uno=1;
		y = x + 1;


		si(x < dos){
			regresar uno
		}
	
		mientras(dos < y ) hacer {
			imprimir(y);
		}

		imprimir(respuesta);
	}
}
