programa test3 {
	prototipo suma(2);
	prototipo resta(2);

	crear entero varglobal;
	varglobal=1;

	funcion entero suma(entero x, entero y){
		crear entero resultado;
		
		resultado =1;
		resultado = x + y;
		regresar resultado
	}

	funcion entero resta(entero x, entero y){
		crear entero resultado;
		
		resultado =1;
		resultado = x - y;
		regresar resultado
	}

	principal(){
		crear entero num1;
		crear entero num2;
		crear entero r;

		varglobal=1;
		num1 = 10;
		num2 = 3;
		r = (suma(1+1,num2);) + (resta(num1,num2););
		
		si(num1>num2){
			imprimir(r);
		}
		
	}
}
